# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=codlabstudio
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/codlabstudio

# JWT Secrets
JWT_SECRET=development_jwt_secret_key_12345
JWT_REFRESH_SECRET=development_refresh_secret_key_12345
JWT_EXPIRES_IN=1h
JWT_REFRESH_EXPIRES_IN=1d

# Application
NODE_ENV=development
PORT=3001
CORS_ORIGIN=http://localhost:3000,http://localhost:8080,https://localhost:8443

# Frontend
NEXT_PUBLIC_API_URL=https://codlabstudio.thegeekybeng.com/api
NEXT_PUBLIC_WS_URL=wss://codlabstudio.thegeekybeng.com

# Security
BCRYPT_ROUNDS=10
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000
EXECUTION_TIMEOUT_MS=10000
MAX_CODE_SIZE_BYTES=1048576
